<view class="flex-col page">
  <view class="flex-col flex-1 group_2">
    <view class="flex-col group_3">
      <view class="flex-row items-center section">
        <image
          class="image_6"
          src="/image/search.png"
        />
        <text class="ml-10 text_2">搜索</text>
      </view>
      <view class="flex-row justify-between items-baseline group_4">
        <text class="font text_3" style="position: relative; left: -75rpx; top: 0rpx">常见运动</text>
        <text class="font_2 text_4 text_5">View All</text>
      </view>
      <view class="flex-row equal-division group_5">
        <view class="flex-col items-center group_6 equal-division-item">
          <image bind:tap="go2run"
            class="image_7"
            src="/image/run.png"
          />
          <text class="mt-14 font_3">跑步</text>
        </view>
        <view class="flex-col items-center group_6 equal-division-item">
          <image
            class="image_7"
            src="/image/swim.png"
          />
          <text class="mt-14 font_3">游泳</text>
        </view>
        <view class="flex-col items-center group_6 equal-division-item">
          <image
            class="image_7"
            src="/image/exercise.png"
          />
          <text class="font_3 mt-15">健身</text>
        </view>
        <view class="flex-col items-center group_6 equal-division-item">
          <image bind:tap="go2cycle"
            class="image_7"
            src="/image/cycle.png"
          />
          <text class="mt-14 font_3">单车</text>
        </view>
      </view>
      <view class="flex-row justify-between items-center group_7">
        <text class="font text_14" style="position: relative; left: -29rpx; top: -3rpx">知名课程</text>
        <text class="font_2 text_4 text_7">View All</text>
      </view>
      <view style="margin: 40rpx auto 0;">
        <image src="/image/boxexercise.png" mode="widthFix"/>
      </view>
      <view class="flex-row justify-between items-center group_10">
        <text class="font text_14" style="position: relative; left: -29rpx; top: -3rpx">健康贴士</text>
        <text class="font_2 text_4 text_15">View All</text>
      </view>
    </view>
    
  </view>
</view>
