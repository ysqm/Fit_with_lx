<view class="container">
  <view class="header">
    <image class="logo" src="/image/avatar1.jpg" />
    <text class="title">Static</text>
  </view>
  <view class="stats">
    <view class="stat-item" wx:for="{{stats}}" wx:key="index">
      <view class="stat-label">{{item.label}}</view>
      <view class="stat-value">{{item.value}}</view>
    </view>
  </view>
  <view class="charts">
    <!-- 这里可以添加图表组件，例如柱状图、饼图等 -->
  </view>
</view>